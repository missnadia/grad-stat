<%= form_for(@student) do |f| %>
  <ul>
    <%= f.label :first_name, "First Name:" %><br>
    <%= f.text_field :first_name %>
  </ul>
  <ul>
    <%= f.label :last_name, "Last Name:" %><br>
    <%= f.text_field :last_name %>
  </ul>
  <ul>
    <%= f.label :email %>:<br>
    <%= f.text_field :email %>
  </ul>
  <ul>
    <%= f.label :password %>:<br>
    <%= f.password_field :password %>
  </ul>
  <ul>
    <%= f.submit %>
  </ul>
<% end %>
